        <licenseUrl>http://autofixture.codeplex.com/license</licenseUrl>
        <projectUrl>http://autofixture.codeplex.com</projectUrl>
    </metadata>
    <files>
        <file src="Ploeh.AutoFixture.AutoNSubstitute.dll" target="lib" />
        <file src="Ploeh.AutoFixture.AutoNSubstitute.XML" target="lib" />
    </files>
</package>